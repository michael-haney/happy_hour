<%= form_for(@explore, html: { class: "form-horizontal", role: "form" }) do |f| %>
  <% if @explore.errors.any? %>
    <div class="alert alert-danger alert-dismissable" role="alert">
      <button type="button" class="close" data-dismiss="alert"><span aria-hidden="true">&times;</span><span class="sr-only">Close</span></button>
      <h4><%= pluralize(@explore.errors.count, "error") %> prohibited this explore from being saved:</h4>

      <ul>
      <% @explore.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="form-group">
    <%= f.label :restaurant_name, class: "col-sm-2 control-label" %>
    <div class="col-sm-10">
      <%= f.text_field :restaurant_name, class: "form-control" %>
    </div>
  </div>
  <div class="form-group">
    <%= f.label :latitude, class: "col-sm-2 control-label" %>
    <div class="col-sm-10">
      <%= f.text_field :latitude, class: "form-control" %>
    </div>
  </div>
  <div class="form-group">
    <%= f.label :longitude, class: "col-sm-2 control-label" %>
    <div class="col-sm-10">
      <%= f.text_field :longitude, class: "form-control" %>
    </div>
  </div>
  <div class="form-group">
    <%= f.label :food_type, class: "col-sm-2 control-label" %>
    <div class="col-sm-10">
      <%= f.text_field :food_type, class: "form-control" %>
    </div>
  </div>
  <div class="form-group">
    <%= f.label :deals, class: "col-sm-2 control-label" %>
    <div class="col-sm-10">
      <%= f.text_field :deals, class: "form-control" %>
    </div>
  </div>
  <div class="form-group">
    <%= f.label :hours, class: "col-sm-2 control-label" %>
    <div class="col-sm-10">
      <%= f.text_field :hours, class: "form-control" %>
    </div>
  </div>
  <div class="form-group">
    <%= f.label :location, class: "col-sm-2 control-label" %>
    <div class="col-sm-10">
      <%= f.text_field :location, class: "form-control" %>
    </div>
  </div>
  <div class="form-group">
    <%= f.label :address, class: "col-sm-2 control-label" %>
    <div class="col-sm-10">
      <%= f.text_field :address, class: "form-control" %>
    </div>
  </div>
  <div class="form-group">
    <div class="col-sm-offset-2 col-sm-10">
      <%= f.submit class: "btn btn-primary" %>
    </div>
  </div>
<% end %>
