<div class="page-header">
  <%= link_to explores_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_explore_path(@explore), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show explore</h1>
</div>

<dl class="dl-horizontal">
  <dt>Restaurant name:</dt>
  <dd><%= @explore.restaurant_name %></dd>

  <dt>Latitude:</dt>
  <dd><%= @explore.latitude %></dd>

  <dt>Longitude:</dt>
  <dd><%= @explore.longitude %></dd>

  <dt>Type:</dt>
  <dd><%= @explore.food_type %></dd>

  <dt>Deals:</dt>
  <dd><%= @explore.deals %></dd>

  <dt>Hours:</dt>
  <dd><%= @explore.hours %></dd>

  <dt>Location:</dt>
  <dd><%= @explore.location %></dd>

  <dt>Address:</dt>
  <dd><%= @explore.address %></dd>

</dl>
